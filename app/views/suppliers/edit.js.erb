$("#modal").dialog({
  autoOpen: true,
  height: 500,
  width: 1000,
  modal: true,
  title: "<%= t 'supplier.supplier' %> - <%= t 'edit' %>",
  open: function() {
    $(this).html("<%= escape_javascript(render('form')) %>")
  },
  buttons: {
    "<%= t 'cancel' %>": function() { $(this).dialog("close"); }
  }
});